  },
  "scripts": {
    "start": "nodemon --exec vsce package",
    "build": "nodemon --watch src src"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Palenight Dev Theme",
        "uiTheme": "vs-dark",
